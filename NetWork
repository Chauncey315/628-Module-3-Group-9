#%%
!pip install pyvis
from pyvis.network import Network
import networkx as nx


#%%
net = Network()

net.add_node(1, label="Node 1")
net.add_node(2)





#%%
CompGraphNX = nx.complete_graph(2)
CompGraph = Network()
# net.enable_physics(True)
CompGraph = CompGraph.from_nx(CompGraphNX)
CompGraph.show("mygraph.html")

# %%
